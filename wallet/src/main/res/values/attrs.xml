<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="Amount">
        <attr name="show_big" format="boolean" />
        <attr name="single_line" format="boolean" />
        <attr name="show_small" format="boolean" />
    </declare-styleable>
    <declare-styleable name="SendOutput">
        <attr name="is_fee" format="boolean" />
    </declare-styleable>
    <declare-styleable name="Address">
        <attr name="multi_line" format="boolean" />
        <attr name="show_coin_icon" format="boolean" />
    </declare-styleable>

    <declare-styleable name="TimingButton">
        <!--结束后的文字-->
        <attr name="tb_psText" format="string" />
        <!--计时总时长，毫秒-->
        <attr name="tb_totalTime" format="integer" />
        <!--计时时间间隔，毫秒-->
        <attr name="tb_timeInterval" format="integer" />
    </declare-styleable>

</resources>